<div id="searchbox">
<%= form_tag "/events/getsearch" do %>
  <p>
    <%= label_tag :lat, "Latitude:" %>
  </p>
  <p>
    <%= text_field_tag :lat, nil, class: "form-control"%>
  </p>
  <p>
    <%= label_tag :lon, "Longitude:" %>
  </p>
  <p>
    <%= text_field_tag :lon, nil, class: "form-control"%>
  </p>
  <p>  
    <%= label_tag :range, "Range:" %>
  </p>
  <p>
    <%= text_field_tag :range, nil, class: "form-control"%>
  </p>
  <p>
    <%= submit_tag "Search", class: "btn btn-default" %>
  </p>
<% end %>
</div>



